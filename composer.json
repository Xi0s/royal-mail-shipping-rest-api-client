{
    "name": "xi0s/royal-mail-shipping-rest-api-client",
    "description": "Royal Mail Shipping REST API Client",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://github.com/Xi0s/royal-mail-shipping-rest-api-client",
    "license": "proprietary",
    "authors": [
        {
            "name": "Swagger and contributors",
            "homepage": "https://github.com/swagger-api/swagger-codegen"
        },
        {
            "name": "zVPS.uk",
            "homepage": "https://zvps.uk"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": { "RoyalMail\\Shipping\\Rest\\Api\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "RoyalMail\\Shipping\\Rest\\Api\\Test\\" : "test/" }
    }
